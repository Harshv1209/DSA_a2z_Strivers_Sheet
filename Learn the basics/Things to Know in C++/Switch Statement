//Solution:-
double areaSwitchCase(int ch, vector<double> a) {
double c,d,precision=.000001;
	switch(ch){
		case 1:
			d =M_PI*(a[0]*a[0]);
			d=round(d / precision) * precision;
			return d;
			
		case 2:
			c =a[0]*a[1];
			c=round(c / precision) * precision;
			return c;
	}
}
